<?xml version="1.0" encoding="UTF-8" ?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <label>Tax Rate</label>
    <pluralLabel>Tax Rates</pluralLabel>
    <visibility>Public</visibility>
    <deploymentStatus>Deployed</deploymentStatus>
    <description
  >Tax rate configuration per country/prefecture with tax mode.</description>
    <fields>
        <fullName>Country__c</fullName>
        <label>Country</label>
        <type>Text</type>
        <length>80</length>
        <required>true</required>
    </fields>
    <fields>
        <fullName>Prefecture__c</fullName>
        <label>Prefecture</label>
        <type>Text</type>
        <length>80</length>
        <required>false</required>
    </fields>
    <fields>
        <fullName>Rate__c</fullName>
        <label>Rate</label>
        <type>Number</type>
        <precision>5</precision>
        <scale>3</scale>
        <required>true</required>
        <description>Tax rate (e.g. 0.100 for 10%)</description>
    </fields>
    <fields>
        <fullName>TaxMode__c</fullName>
        <label>Tax Mode</label>
        <type>Picklist</type>
        <valueSet>
            <valueSetDefinition>
                <sorted>true</sorted>
                <value><fullName>TaxIncluded</fullName><default
          >true</default><label>Tax Included</label></value>
                <value><fullName>TaxExcluded</fullName><default
          >false</default><label>Tax Excluded</label></value>
            </valueSetDefinition>
        </valueSet>
        <required>true</required>
    </fields>
</CustomObject>
